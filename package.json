{
  "name": "formpi",
  "version": "0.1.3",
  "description": "FormPI: A Vue Form component library/form builder. Using Tailwind CSS and Headless UI.",
  "exports": {
    "import": "./dist/formpi.mjs",
    "require": "./dist/formpi.js"
  },
  "files": [
    "dist",
    "locale"
  ],
  "scripts": {
    "dev": "bunx --bun vite build --watch",
    "build": "bunx --bun vite build",
    "locales": "node ./scripts/locales.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitOfGold/formpi.git"
  },
  "keywords": [
    "vue",
    "form",
    "component",
    "library",
    "builder",
    "tailwind",
    "css"
  ],
  "author": "László Matuska @BitOfGold",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/BitOfGold/formpi/issues"
  },
  "homepage": "https://github.com/BitOfGold/formpi#readme",
  "dependencies": {
    "@vee-validate/i18n": "^4.12.2",
    "@vee-validate/rules": "^4.12.2",
    "@vueup/vue-quill": "^1.2.0",
    "oh-vue-icons": "^1.0.0-rc3",
    "tailwind-merge": "^2.1.0",
    "vee-validate": "^4.12.2",
    "vue": "^3.3.9"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.1",
    "sass": "^1.69.5",
    "vite": "^5.0.4",
    "vite-plugin-css-injected-by-js": "^3.3.0"
  }
}
